    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0-1",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-register": "^6.11.6",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "esdoc": "0.4.7",
